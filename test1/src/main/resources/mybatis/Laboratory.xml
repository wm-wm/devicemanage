<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.yuan.dao.LaboratoryMapper">
    <select id="laboratoryByLid" resultType="laboratory">
        select *
        from laboratory
        where lid = #{lid}
    </select>

</mapper>